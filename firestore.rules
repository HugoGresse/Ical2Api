rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /events/{eventId} {
      allow read: if true;
    }

    match /icals/{icalId} {
      allow read: if true;
    }

    match /reminder/{reminderId} {
      allow read: if true;
    }

    match /organizations/{organizationId} {
      allow read: if true;
    }

    match /organizationsPrivateData/{orgPrivateDataId} {
      allow read: if false;
    }
  }
}
